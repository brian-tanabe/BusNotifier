    </bean>

    <bean id="401-response" class="com.bttanabe.busnotifier.test.factories.FileStringFactory">
        <property name="inputFile" value="classpath:/test-responses/401-response.json"/>
    </bean>

    <bean id="404-response" class="com.bttanabe.busnotifier.test.factories.FileStringFactory">
        <property name="inputFile" value="classpath:/test-responses/404-response.json"/>
    </bean>

    <bean id="429-response" class="com.bttanabe.busnotifier.test.factories.FileStringFactory">
        <property name="inputFile" value="classpath:/test-responses/429-response.json"/>
    </bean>

    <bean id="500-response" class="com.bttanabe.busnotifier.test.factories.FileStringFactory">
        <property name="inputFile" value="classpath:/test-responses/500-response.json"/>
