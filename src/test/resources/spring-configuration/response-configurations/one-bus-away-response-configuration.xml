<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- valid response URLs -->
    <bean id="arrivalsAndDeparturesForStop1_12378" class="com.bttanabe.busnotifier.test.factories.FileStringFactory">
        <property name="inputFile"
                  value="classpath:/test-responses/arrivals-and-departures-for-stop-1_12378.json"/>
    </bean>

    <bean id="stopForLocation-47.62-122.29" class="com.bttanabe.busnotifier.test.factories.FileStringFactory">
        <property name="inputFile" value="classpath:/test-responses/stops-for-location-47.62-122.29.json"/>
    </bean>

    <!-- Error handling URLs -->
    <bean id="400-response" class="com.bttanabe.busnotifier.test.factories.FileStringFactory">
